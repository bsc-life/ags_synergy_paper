                <drug_X_target_conc conserved="false" units="mM" description="">0.683e-03</drug_X_target_conc> 
                
                
                <drug_Y_mapping_type conserved="false" units="dimensionless" description="">0</drug_Y_mapping_type> 
                <drug_Y_permeability conserved="false" units="cm/s" description="">4.8029529999999993e-05</drug_Y_permeability>
