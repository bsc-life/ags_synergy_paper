        <drug_Y_pulse_duration type="double" units="min" description="">40</drug_Y_pulse_duration>
        <drug_Y_pulse_concentration type="double" units="mM" description="">15.5e-06</drug_Y_pulse_concentration>
        <drug_Y_pulse_frequency type="double" units="min" description="Frequency of pulses">40</drug_Y_pulse_frequency>
        <drug_Y_target type="string" units="min" description="">anti_MEK</drug_Y_target>
        <drug_Y_membrane_length type="double" units="" description=""> 100 </drug_Y_membrane_length>

        
        <new_intracellular_dt type="double" units="min" description="">15.016958863753642</new_intracellular_dt>
        <new_scaling type="double" units="dimensionless" description="">604.4175419766784</new_scaling>

        
        <use_3D_setup type="bool" units="dimensionless" description="Enable 3D cell distribution">true</use_3D_setup>
        <z_height type="double" units="um" description="Height of cylindrical cell distribution">100.0</z_height>
        <sphere_distribution type="bool" units="dimensionless" description="Use spherical instead of cylindrical distribution">false</sphere_distribution>


        
